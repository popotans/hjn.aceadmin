    <script type="text/javascript">
        function laywin(title, url, width, height) {
            var index = layer.open({
                title: title,
                type: 2,
                area: [width, height],
                fix: true, //不固定
                maxmin: true,
                content: url
            });
        }

        function laymsgsuc(msg, calback) {
            layer.alert(msg, {
                icon: 1,
            }, calback);
        }

        function laymsgfal(msg, calback) {
            layer.alert(msg, {
                icon: 2,
            }, calback);
        }
        //toastr.options.positionClass = 'toast-bottom-right';
        $(function () {
            function tojson(ddddd) {
                return eval('(' + ddddd + ')');
            }

            $.get("@Url.Action("GetMenus","AdmMenu")?1211", {}, function (ddddd, textStatus) {
                //alert(ddddd);
                $('#menu').sidebarMenu({
                    data: tojson(ddddd)
                });
            });

            $(".topcontent2").click(function () {
                $.get("menu2.html?1211", {}, function (ddddd, textStatus) {
                    //alert(ddddd);
                    $('#menu').empty();
                    $('#menu').sidebarMenu({
                        data: tojson(ddddd)
                    });
                });
            });

            $(".topcontent").click(function () {
                $.get(" @Url.Action("GetMenus","AdmMenu")", {}, function (ddddd, textStatus) {
                    //alert(ddddd);
                    $('#menu').empty();
                    $('#menu').sidebarMenu({
                        data: tojson(ddddd)
                    });
                });
            });
        });
    </script>
